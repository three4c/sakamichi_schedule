generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model accounts {
  id                  String  @id
  user_id             String
  type                String
  provider            String
  provider_account_id String
  refresh_token       String? @db.Text
  access_token        String? @db.Text
  expires_at          Int?
  token_type          String?
  scope               String?
  id_token            String? @db.Text
  session_state       String?

  @@unique([provider, provider_account_id])
  @@index([user_id])
}

model groups {
  id         String  @id
  user_id    String
  color_id   String  @unique
  order      Int
  ticketData String?

  @@index([user_id])
}

model sessions {
  id            String   @id
  session_token String   @unique
  user_id       String
  expires       DateTime

  @@index([user_id])
}

model users {
  id             String    @id
  name           String?
  email          String?   @unique
  email_verified DateTime?
  image          String?
}

model verificationtokens {
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}

model colors {
  id   String @id
  name String
}

model members {
  id       String @id
  kanji    String
  hiragana String
  src      String
  href     String
  color_id String @unique
}

model tickets {
  id       String @id
  href     String
  date     String
  text     String
  color_id String @unique
}
