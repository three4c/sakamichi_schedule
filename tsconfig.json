    "target": "es2016",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "rootDir": "./",
    "resolveJsonModule": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
